{
  "hosting": {
    "public": "build",
    "headers": [ {
      "source": "**",
      "headers": [ {
        "key": "Cache-Control",
        "value": "no-cache, no-store, must-revalidate"
      }]
    }, {
      "source": "**",
      "headers": [ {
        "key": "Pragma",
        "value": "no-cache"
      }]
    }, {
      "source": "**",
      "headers": [ {
        "key": "Expires",
        "value": "0"
      }]
    }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    }]
  },
  "functions": {
    "source": "functions",
    "codebase": "default",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log"
    ]
  }
}
